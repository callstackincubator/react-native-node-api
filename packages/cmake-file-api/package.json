{
  "name": "cmake-file-api",
  "version": "0.1.0",
  "type": "module",
  "description": "TypeScript wrapper around the CMake File API",
  "homepage": "https://cmake.org/cmake/help/latest/manual/cmake-file-api.7.html",
  "scripts": {
    "build": "tsc --build",
    "lint": "eslint 'src/**/*.ts'",
